<h3><%= title %></h3>

<input class="input-block-level" type="text" placeholder="Type a filter here, e.g. 'Grading option'" id="search_text">

<% _.each(items, function(item) { %>
<h4>Professor: <%= item.name %></h4>

<table class="table table-bordered table-hover">
    <tbody>
        <tr>
            <td>Course title</td>
            <td><%= item.course_title %></td>
        </tr>
        <tr>
            <td>Course number</td>
            <td><%= item.course_number %></td>
        </tr>
        <tr>
            <td>First choices</td>
            <td>
                <ul>
                    <% _.each(item.first_choices, function(choice) { %>
                        <li><%= choice %></li>
                    <% }); %>
                </ul>
            </td>
        </tr>
        <tr>
            <td>Second choices</td>
            <td>
                <ul>
                    <% _.each(item.second_choices, function(choice) { %>
                        <li><%= choice %></li>
                    <% }); %>
                </ul>
            </td>
        </tr>
        <tr>
            <td>Third choices</td>
            <td>
                <ul>
                    <% _.each(item.third_choices, function(choice) { %>
                        <li><%= choice %></li>
                    <% }); %>
                </ul>
            </td>
        </tr>
        <tr>
            <td>Other additional availability</td>
            <td><%= item.other_additional_availability %></td>
        </tr>
        <tr>
            <td>Other preferred times</td>
            <td><%= item.other_preferred_times %></td>
        </tr>
        <tr>
            <td>Unavailable times</td>
            <td>
                <ul>
                    <% _.each(item.unavailable_times, function(time) { %>
                        <li><%= time %></li>
                    <% }); %>
                </ul>
            </td>
        </tr>
        <tr>
            <td>Shouldn't conflict with these courses</td>
            <td><%= item.no_conflict_courses %></td>
        </tr>
        <tr>
            <td>Other comments</td>
            <td><%= item.other_comments %></td>
        </tr>
        <tr>
            <td>Max students</td>
            <td><%= item.max_students %></td>
        </tr>
        <tr>
            <td>Is special permission required?</td>
            <td><%= item.is_special_permission_required %></td>
        </tr>
        <tr>
            <td>Is attendance of first class mandatory?</td>
            <td><%= item.is_first_class_attendance_mandatory %></td>
        </tr>
        <tr>
            <td>Grading option</td>
            <td><%= item.grading_option %></td>
        </tr>
        <tr>
            <td>Final exam</td>
            <td><%= item.final_exam %></td>
        </tr>
        <tr>
            <td>Are the prerequisites mandatory?</td>
            <td><%= item.prequisites_are_mandatory %></td>
        </tr>
        <tr>
            <td>If teaching 314, 357, 358, or 359 then list additional prerequisites</td>
            <td><%= item.additional_prerequisites %></td>
        </tr>
        <tr>
            <td>Room or building choices</td>
            <td>
                <ul>
                    <% _.each(item.room_or_building_choices, function(choice) { %>
                        <li><%= choice %></li>
                    <% }); %>
                </ul>
            </td>
        </tr>
        <tr>
            <td>Equipment/facilities needed</td>
            <td>
                <ul>
                    <% _.each(item.equipment_facilities_needed, function(need) { %>
                        <li><%= need %></li>
                    <% }); %>
                </ul>
            </td>
        </tr>
        <tr>
            <td>Additional requests/comments</td>
            <td><%= item.additional_comments %></td>
        </tr>
    </tbody>
</table>

<hr />
<% }); %>